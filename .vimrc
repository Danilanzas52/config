set rtp+=/usr/local/lib/python2.7/dist-packages/powerline/bindings/vim/
" Always show statusline
set laststatus=2
" Use 256 colours (Use this setting only if your terminal supports 256 colours)
set t_Co=256






















:imap jj <Esc>


syntax on
set tabstop=4
set softtabstop=4
set expandtab
set shiftwidth=4
set ai
set number
set relativenumber
set hlsearch
set ruler
highlight Comment ctermfg=green
